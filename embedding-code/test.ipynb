    "#-*-coding:utf-8-*-\n",
    "import sys;\n",
    "total=10000\n",
    "for i in range(0,total):\n",
    "  percent=float(i)*100/float(total)\n",
    "  sys.stdout.write(\"%.2f%%\"%percent);\n",
    "  sys.stdout.write(\"\\r\");\n",
    "  sys.stdout.flush();\n",
    "sys.stdout.write(\"100%!finish!\\r\");\n",
    "sys.stdout.flush();"
